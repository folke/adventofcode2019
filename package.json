        "eslint-plugin-import": "^2.19.1",
        "eslint-plugin-jest": "^23.1.1",
        "eslint-plugin-prettier": "^3.1.2",
        "jest": "^24.9.0",
        "prettier": "^1.19.1",
        "prettier-plugin-packagejson": "^2.0.6",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.4"
