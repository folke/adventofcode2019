{
    "name": "adventofcode",
    "version": "0.0.0",
    "author": "Folke @folke",
    "bin": {
        "adventofcode": "./bin/run"
    },
    "bugs": "https://github.com/folke/adventofcode/issues",
    "dependencies": {
        "@oclif/command": "^1",
        "@oclif/config": "^1",
        "@oclif/plugin-help": "^2",
        "tslib": "^1"
    },
    "devDependencies": {
        "@oclif/dev-cli": "^1",
        "@oclif/test": "^1",
        "@types/chai": "^4",
        "@types/mocha": "^5",
        "@types/node": "^10",
        "@typescript-eslint/eslint-plugin": "^2.11.0",
        "@typescript-eslint/parser": "^2.11.0",
        "chai": "^4",
        "eslint": "^6.7.2",
        "eslint-config-oclif": "^3.1",
        "eslint-config-oclif-typescript": "^0.1",
        "eslint-config-prettier": "^6.7.0",
        "eslint-plugin-prettier": "^3.1.2",
        "globby": "^10",
        "mocha": "^5",
        "nyc": "^14",
        "prettier": "^1.19.1",
        "ts-node": "^8",
        "typescript": "^3.3"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "homepage": "https://github.com/folke/adventofcode",
    "keywords": [
        "oclif"
    ],
    "license": "MIT",
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "adventofcode",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "repository": "folke/adventofcode",
    "scripts": {
        "postpack": "rm -f oclif.manifest.json",
        "posttest": "eslint . --ext .ts --config .eslintrc",
        "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
        "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
        "version": "oclif-dev readme && git add README.md",
        "eslint": "eslint src/**/*.ts"
    },
    "types": "lib/index.d.ts"
}
